{
    "name": "LATE Extension",
    "manifest_version": 2,
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["submitty.js"],
            "css": ["submitty.css"]
        }
    ],
    "background": {
        "js": ["background.js"]
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs",
        "storage"
    ],
    "web_accessible_resources": [
        "res/late.png"  
    ] 
}